# Set to 'true' to use built-in mock/demo data (no external calls).
DEMO=true

# FoxESS Cloud API (provide either a permanent token or app credentials for signed requests)
FOXESS_BASE_URL=https://www.foxesscloud.com
FOXESS_TOKEN=put_your_token_here

# If you use signed requests instead of a static token, fill these (optional, leave empty if using FOXESS_TOKEN)
FOXESS_APP_ID=
FOXESS_APP_SECRET=

# Poland Market Price (RCE)
# If you have your own data source endpoint that returns hourly prices in JSON, set it here.
RCE_JSON_URL=

FOXESS_SN=put_your_sn_here


# Urządzenie (SN) – wymagane przy realnym połączeniu
FOXESS_DEVICE_SN=
# Strefa czasowa do prezentacji (opcjonalnie)
FOXESS_TIMEZONE=Europe/Warsaw
